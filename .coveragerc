[run]
branch = true
source = genrss
omit =
    .venv
    __meta__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug:
    if settings.DEBUG
    raise NotImplementedError
    if 0:
    if False:
    if __name__ == .__main__.:
    @abstractmethod

fail_under = 80
precision = 2
show_missing = true
skip_covered = true

[html]
directory = tests/htmlcov
